ledIndex = ledIndex % 32;

if(ledIndex == 31)
{
turn ++;
turn = turn%3;
}


var r = getDeviceRed(0,ledIndex);
var g = getDeviceGreen(0,ledIndex);
var b = getDeviceBlue(0,ledIndex);

//resetLED(ledIndex)

lights[0] = r;
lights[1] = g;
lights[2] = b;

lights[clamp(turn,0,2)] += 20;

switch(turn)
{
    case 0:
    lights[1] = 0;
    break;
    
    case 1:
    lights[2] = 0;
    break;
    
    case 2:
    lights[0] = 0;
    break;
    
