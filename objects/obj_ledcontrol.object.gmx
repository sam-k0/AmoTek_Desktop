brightness = 1; // all rgb values get multiplied by this
audioMultiplier = 1; // peak val gets multiplied by this
deviceSlot = 0;
// audiobars
maxErrors = 5;
maxAudioLed = 0;

// Cycle
turn = 0;
ledIndex = 0;

// Ambi
AMBICHECKCOOLDOWN= 32;
ambiCheckCooldown = AMBICHECKCOOLDOWN;

// audio visualizer
audioPeak = ar_get_peak();
ledlist = ds_list_create();

