leftrgb[0] = 200;
leftrgb[1] = 0;
leftrgb[2]= 0;

rightrgb[0] = 0;
rightrgb[1] = 0;
rightrgb[2] = 200;

savedrgb[0,0] = leftrgb[0]
savedrgb[0,1] = leftrgb[1]
savedrgb[0,2] = leftrgb[2]
savedrgb[1,0] = rightrgb[0]
savedrgb[2,1] = rightrgb[1]
savedrgb[3,2] = rightrgb[2]


// Color registers
colors[0,0] = 0;
colors[0,1] = 0;
colors[0,2] = 0;
// Fill colors
for(var xx = 0; xx &lt; 32; xx++)
{
    for(var yy = 0; yy &lt; 2; yy++)
    {
        colors[xx,yy] = 0;
    }
}

